{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
      "jsx-no-lambda": false,
      "no-console": false,
      "interface-name": false,
      "class-name": false,
      "interface-over-type-literal": false
    },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/lcov-report/*.js"
    ]
  }
}
